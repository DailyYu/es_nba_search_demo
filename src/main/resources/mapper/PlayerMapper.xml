<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yutao.nba_search.dao.PlayerDAO">
    <select id="listAllPlayer" resultType="com.yutao.nba_search.model.Player">
        SELECT * FROM nba;
    </select>

    <select id="listPlayer" resultType="com.yutao.nba_search.model.Player">
        SELECT * FROM nba LIMIT #{offset}, #{pageSize};
    </select>

</mapper>


